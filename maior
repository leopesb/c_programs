#include <stdio.h>

int main(){
    int numbers[5]; /* array to be sorted */
    int i;  /* a variable for the loop */
    int maior; /* auxiliary variable for swaps */
    
    numbers[0] = -999;

    for(i = 1; i < 10; i++) {
        printf("Type an integer number: ");
        scanf("%d", &numbers[i]);
    
        if(numbers[i] > maior){
            maior = numbers[i];
            
        printf("%d is the new largest number!\n", numbers[i]);
        }
    }
    return 0;
}
